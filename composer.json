{
    "name": "iq2i/ai-maker-bundle",
    "type": "symfony-bundle",
    "description": "Maker bundle boosted by AI",
    "keywords": ["ai", "generator", "code generator", "dev", "mistral"],
    "homepage": "https://github.com/IQ2i/ai-maker-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lo√Øc Sapone",
            "email": "loic@sapone.fr",
            "homepage": "https://loic-sapone.fr"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-client": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0"
    },
    "require-dev": {
        "doctrine/orm": "^3.3",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.1",
        "rector/rector": "^2.0.12",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/maker-bundle": "^1.63"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "IQ2i\\AiMakerBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "IQ2i\\AiMakerBundle\\Tests\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
